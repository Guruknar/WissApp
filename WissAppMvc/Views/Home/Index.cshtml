@model HomeIndexViewModel

<div class="row" id="d_users">
    <div class="col-md-2">
        <ul class="nav nav-pills nav-stacked">
            @foreach(var user in Model.Users)
            {
                <li><a href="#@user.UserId">@user.UserName <span class="badge badge-light">@user.MessageCount</span></a></li>
            }
        </ul>
    </div>
    <div class="col-md-10">

    </div>
</div>